{
  "name": "dashboard-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@johmun/vue-tags-input": "^1.5.9",
    "axios": "^0.18.0",
    "buefy": "^0.7.0",
    "chart.js": "^2.7.2",
    "chartjs-plugin-labels": "^1.1.0",
    "crypto-browserify": "^3.11.0",
    "crypto-js": "^3.1.9-1",
    "element-resize-detector": "^1.1.14",
    "google-palette": "^1.1.0",
    "html2canvas": "^1.0.0-alpha.12",
    "interactjs": "^1.3.4",
    "jspdf": "^1.4.1",
    "lodash": "^4.17.10",
    "vue": "^2.5.17",
    "vue-chartjs": "^3.4.0",
    "vue-grid-layout": "^2.2.0",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1",
    "vue-search-select": "^2.7.0",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@types/chart.js": "^2.7.31",
    "@types/lodash": "^4.14.118",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-airbnb": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "eslint-plugin-vue": "4.7.1",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "max-len": ["warn", {"ignoreComments": true, "code": 120}],
      "prefer-destructuring": ["error", {"array": false, "object": true}, {"enforceForRenamedProperties": false}],
      "no-param-reassign": ["error", {"props": false}],
      "function-paren-newline": ["warn", "consistent"],
      "no-underscore-dangle": ["error", {"allowAfterThis": true}],
      "arrow-parens": ["error", "always"],
      "no-unused-vars": ["warn", {"vars": "all","args": "none","ignoreRestSiblings": true}],
      "quote-props": ["warn", "consistent"],
      "object-curly-newline": "off",
      "no-multi-spaces": ["warn", { "exceptions": { "Property": true, "VariableDeclarator": true, "ImportDeclaration": true } }],
      "no-mixed-operators": ["error", {"allowSamePrecedence": false, "groups": [
          ["*","/", "%"], ["%", "**"],
          ["&", "|"], ["^", "~"], ["<<", ">>", ">>>"],
          ["==", "!=", "===", "!==", ">", ">=", "<", "<="],
          ["&&", "||"],
          ["in", "instanceof"]
        ]
      }]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "transformIgnorePatterns": [
      "node_modules/(?!(babel-jest|jest-vue-preprocessor)/)"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
